{
  "name": "@pancakeswap/ui",
  "version": "0.0.1",
  "sideEffects": [
    "*.css",
    "*.css.ts"
  ],
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.0",
    "@vanilla-extract/css": "^1.10.0",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/sprinkles": "^1.5.2",
    "clsx": "^1.2.1",
    "deepmerge": "^4.0.0",
    "tslib": "^2.2.0"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@vanilla-extract/esbuild-plugin": "^2.3.0",
    "react": "^18.2.0",
    "tsup": "^6.7.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./css": {
      "types": "./dist/css/index.d.ts",
      "import": "./dist/css.mjs",
      "require": "./dist/css.js"
    },
    "./index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  }
}
