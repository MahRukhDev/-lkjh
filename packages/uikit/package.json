{
  "name": "@pancakeswap/uikit",
  "version": "0.63.1",
  "description": "Set of UI components for pancake projects",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/pancakeswap/pancake-toolkit/tree/master/packages/pancake-uikit",
  "license": "MIT",
  "scripts": {
    "start": "yarn storybook",
    "build:lib": "tsup src/index.ts --format esm,cjs --dts --external react",
    "watch": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check --loglevel error 'src/**/*.{js,jsx,ts,tsx}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.5.0",
    "@storybook/addon-actions": "^6.5.0",
    "@storybook/addon-essentials": "^6.5.0",
    "@storybook/addon-links": "^6.5.0",
    "@storybook/builder-vite": "^0.2.0",
    "@storybook/react": "^6.5.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^12.1.3",
    "@types/lodash": "^4.14.168",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "jest-styled-components": "^7.0.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.3",
    "themeprovider-storybook": "^1.7.2",
    "tsconfig": "*",
    "ts-jest": "^27.1.3",
    "vite": "^3.0.0",
    "tsup": "^5.10.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.2.3"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "@types/styled-system": "^5.1.15",
    "framer-motion": "6.5.1",
    "lodash": "^4.17.20",
    "react-popper": "^2.2.5",
    "styled-system": "^5.1.5",
    "tslib": "^2.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
