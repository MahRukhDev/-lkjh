{
  "name": "@pancakeswap/awgmi",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "dependencies": {
    "@ethersproject/units": "^5.7.0",
    "@tanstack/react-query": "^4.2.3",
    "@tanstack/react-query-persist-client": "^4.2.1",
    "@tanstack/query-sync-storage-persister": "^4.2.3",
    "eventemitter3": "^4.0.7",
    "use-sync-external-store": "^1.2.0",
    "zustand": "^4.1.1"
  },
  "peerDependencies": {
    "@blocto/sdk": "^0.3.0-beta.9",
    "aptos": "^1.3.16",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@blocto/sdk": "^0.3.0-beta.9",
    "tsup": "^6.2.3",
    "aptos": "^1.3.16",
    "vitest": "^0.23.1"
  },
  "files": [
    "/core/dist",
    "/dist"
  ],
  "scripts": {
    "build:src": "tsup src/index.ts --format esm,cjs --dts",
    "build:core": "tsup core/src/index.ts --format esm,cjs --dts --outDir core/dist",
    "dev:src": "tsup src/index.ts --format esm,cjs --dts --watch",
    "dev:core": "tsup core/src/index.ts --format esm,cjs --dts --outDir core/dist --watch",
    "dev": "yarn dev:src & yarn dev:core",
    "build": "yarn build:core && yarn build:src",
    "test": "vitest --run",
    "prepublishOnly": "yarn run build:all"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./core/dist/index.d.ts",
      "import": "./core/dist/index.mjs",
      "require": "./core/dist/index.js"
    }
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@blocto/sdk": {
      "optional": true
    }
  }
}
