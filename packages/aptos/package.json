{
  "name": "@pancakeswap/aptos",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@tanstack/react-query": "^4.2.3",
    "@tanstack/react-query-persist-client": "^4.2.1",
    "@tanstack/query-sync-storage-persister": "^4.2.3",
    "use-sync-external-store": "^1.2.0"
  },
  "peerDependencies": {
    "aptos": "^1.3.11"
  },
  "devDependencies": {
    "aptos": "^1.3.11",
    "vitest": "^0.23.1"
  },
  "scripts": {
    "build:src": "tsup src/index.ts --format esm,cjs --dts",
    "build:core": "tsup core/index.ts --format esm,cjs --dts --outDir core/dist",
    "build:all": "yarn build:core && yarn build:src",
    "prepublishOnly": "yarn run build:all"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./core/dist/index.d.ts",
      "import": "./core/dist/index.mjs",
      "require": "./core/dist/index.js"
    }
  }
}
