{
  "name": "pancake-frontend",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "apis/*"
  ],
  "scripts": {
    "dev": "turbo run dev --filter=web",
    "build": "turbo run build --filter=web",
    "start": "turbo run start --filter=web",
    "test": "turbo run test",
    "postinstall": "turbo run --filter=web postinstall",
    "lint": "turbo run lint --filter=web",
    "format:check:staged": "turbo run format:check:staged --filter=web",
    "updateLPsAPR": "yarn turbo run build --filter=@pancakeswap/sdk && NODE_PATH=./apps/web/src ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/updateLPsAPR.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "turbo": "^1.4.5"
  }
}
