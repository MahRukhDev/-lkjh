{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/develop",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "format:check": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "build:list": {
      "outputs": ["list/**"]
    },
    "integration-test": {
      "outputs": [],
      "inputs": ["cypress/integration/**.test.ts"]
    },
    "format:check:staged": {
      "cache": false
    },
    "postinstall": {
      "cache": false
    }
  }
}
