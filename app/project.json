{
  "root": "app",
  "sourceRoot": "app",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-script",
      "options": {
        "script": "build"
      },
      "outputs": ["app/.next", "packages/uikit/dist"],
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ]
    },
    "lint": {
      "executor": "@nrwl/workspace:run-script",
      "options": {
        "script": "lint"
      },
      "outputs": [],
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ]
    },
    "test": {
      "executor": "@nrwl/workspace:run-script",
      "options": {
        "script": "test"
      },
      "outputs": [],
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ]
    }
  }
}
